nlineClass, {disabled: disabled}]\">\n    <label :for=\"cbId\" class=\"form-check-label\">\n      <input :id=\"cbId\"\n             class=\"form-check-input\"\n             type=\"radio\"\n             :disabled=\"disabled\"\n             :value=\"label\"\n             v-model=\"model\" />\n      <span class=\"form-check-sign\">\n      </span>\n      <slot></slot>\n    </label>\n  </div>\n</template>\n<script>\n  export default {\n    name: 'n-radio',\n    props: {\n      label: [String, Number],\n      disabled: Boolean,\n      value: [String, Boolean],\n      inline: Boolean\n    },\n    data () {\n      return {\n        cbId: ''\n      }\n    },\n    computed: {\n      model: {\n        get () {\n          return this.value\n        },\n        set (value) {\n          this.$emit('input', value)\n        }\n      },\n      inlineClass () {\n        if (this.inline) {\n          return `form-check-inline`\n        }\n        return ''\n      }\n    },\n    created () {\n      this.cbId = Math.random().toString(16).slice(2)\n    }\n  }\n</script>\n<style lang=\"scss\" scoped>\n  @import \"~@/assets/sass/now-ui-kit/variables\";\n  @import \"~@/assets/sass/now-ui-kit/checkboxes-radio\";\n</style>\n"],"sourceRoot":"src/components/Inputs"}]}